{
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**",
        "public/dist/**"
      ],
      "dependsOn": [
        "^build"
      ],
      "env": ["SOME_ENV_VAR"]
    },
    "test": {
      "outputs": [
        "coverage/**"
      ],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
